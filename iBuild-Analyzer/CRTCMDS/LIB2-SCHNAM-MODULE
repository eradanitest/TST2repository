-include makefile_defs

SCHNAM.MODULE : QRPGLESRC.FILE/SCHNAM.MBR \
    CUSMSTL3.FILE \
    SCHNAMD.FILE
	# DBGVIEW(*STMT)
	# TGTRLS(V7R4M0)
ifdef LIB2-SCHNAM-MODULE-PARMS
	$(eval @_PARMS = $(LIB2-SCHNAM-MODULE-PARMS))
else
	$(eval @_PARMS = $(CRTRPGMODPARMS))
endif
	system -v -k "CRTRPGMOD $(@_PARMS)  SRCFILE(QRPGLESRC) SRCMBR(SCHNAM) TGTCCSID(37) PRFDTA(*NOCOL) SRTSEQ(*HEX) LANGID(*JOBRUN) MODULE($(LIB2)/SCHNAM) STGMDL(*INHERIT)"

CUSMSTL3.FILE : QDDSSRC.FILE/CUSMSTL3.MBR \
    CUSMST.FILE
	system -v -k "EDOCRTOBJ BLDTGT($(LIB2)/CUSMSTL3) OBJTYPE(*FILE) OBJATTR(LF) CMD(CRTLF) CMDPARMS(' FILE($(LIB2)/CUSMSTL3) SRCFILE(QDDSSRC) SRCMBR(CUSMSTL3) TEXT(''Customer Master L3'')')"

CUSMST.FILE : QDDSSRC.FILE/CUSMST.MBR
	system -v -k "EDOCRTOBJ BLDTGT($(LIB2)/CUSMST) OBJTYPE(*FILE) OBJATTR(PF) CMD(CRTPF) CMDPARMS(' FILE($(LIB2)/CUSMST) SRCFILE(QDDSSRC) SRCMBR(CUSMST) TEXT(''Customer Master'')')"

SCHNAMD.FILE : QDDSSRC.FILE/SCHNAMD.MBR \
    CUSMSTL3.FILE
	system -v -k "EDOCRTOBJ BLDTGT($(LIB2)/SCHNAMD) OBJTYPE(*FILE) OBJATTR(DSPF) CMD(CRTDSPF) CMDPARMS(' FILE($(LIB2)/SCHNAMD) SRCFILE(QDDSSRC) SRCMBR(SCHNAMD) DFRWRT(*YES) RSTDSP(*NO) TEXT(''Customer Search by Name'')')"

