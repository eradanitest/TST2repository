-include makefile_defs

CUSMSTL1.FILE : QDDSSRC.FILE/CUSMSTL1.MBR \
    CUSMST.FILE
	system -v -k "EDOCRTOBJ BLDTGT($(LIB2)/CUSMSTL1) OBJTYPE(*FILE) OBJATTR(LF) CMD(CRTLF) CMDPARMS(' FILE($(LIB2)/CUSMSTL1) SRCFILE(QDDSSRC) SRCMBR(CUSMSTL1) TEXT(''Customer Master L1'')')"

CUSMST.FILE : QDDSSRC.FILE/CUSMST.MBR
	system -v -k "EDOCRTOBJ BLDTGT($(LIB2)/CUSMST) OBJTYPE(*FILE) OBJATTR(PF) CMD(CRTPF) CMDPARMS(' FILE($(LIB2)/CUSMST) SRCFILE(QDDSSRC) SRCMBR(CUSMST) TEXT(''Customer Master'')')"

