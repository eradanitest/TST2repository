-include makefile_defs

CUSMAIN.MODULE : QRPGLESRC.FILE/CUSMAIN.MBR \
    MAINMENU.FILE
	# DBGVIEW(*STMT)
	# TGTRLS(V7R4M0)
ifdef LIB2-CUSMAIN-MODULE-PARMS
	$(eval @_PARMS = $(LIB2-CUSMAIN-MODULE-PARMS))
else
	$(eval @_PARMS = $(CRTRPGMODPARMS))
endif
	system -v -k "CRTRPGMOD $(@_PARMS)  SRCFILE(QRPGLESRC) SRCMBR(CUSMAIN) TGTCCSID(37) PRFDTA(*NOCOL) SRTSEQ(*HEX) LANGID(*JOBRUN) MODULE($(LIB2)/CUSMAIN) STGMDL(*INHERIT)"

MAINMENU.FILE : QDDSSRC.FILE/MAINMENU.MBR
	system -v -k "EDOCRTOBJ BLDTGT($(LIB2)/MAINMENU) OBJTYPE(*FILE) OBJATTR(DSPF) CMD(CRTDSPF) CMDPARMS(' FILE($(LIB2)/MAINMENU) SRCFILE(QDDSSRC) SRCMBR(MAINMENU) DFRWRT(*YES) RSTDSP(*NO)')"

