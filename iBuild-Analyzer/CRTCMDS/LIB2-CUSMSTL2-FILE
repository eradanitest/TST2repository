-include makefile_defs

CUSMSTL2.FILE : QDDSSRC.FILE/CUSMSTL2.MBR \
    CUSMST.FILE
	system -v -k "EDOCRTOBJ BLDTGT($(LIB2)/CUSMSTL2) OBJTYPE(*FILE) OBJATTR(LF) CMD(CRTLF) CMDPARMS(' FILE($(LIB2)/CUSMSTL2) SRCFILE(QDDSSRC) SRCMBR(CUSMSTL2) TEXT(''Customer Master L2'')')"

CUSMST.FILE : QDDSSRC.FILE/CUSMST.MBR
	system -v -k "EDOCRTOBJ BLDTGT($(LIB2)/CUSMST) OBJTYPE(*FILE) OBJATTR(PF) CMD(CRTPF) CMDPARMS(' FILE($(LIB2)/CUSMST) SRCFILE(QDDSSRC) SRCMBR(CUSMST) TEXT(''Customer Master'')')"

