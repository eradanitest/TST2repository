-include makefile_defs

CUSTMAINT.CMD : QCMDSRC.FILE/CUSTMAINT.MBR
ifdef LIB2-CUSTMAINT-CMD-PARMS
	$(eval @_PARMS = $(LIB2-CUSTMAINT-CMD-PARMS))
else
	$(eval @_PARMS = $(CRTCMDPARMS))
endif
	system -v -k "CRTCMD $(@_PARMS) CMD($(LIB2)/CUSTMAINT) PGM(*LIBL/CUSTMAINT) SRCFILE(QCMDSRC) SRCMBR(CUSTMAINT) TEXT('Customer Maintenance') MODE(*PROD *DEBUG *SERVICE) ALLOW(*BPGM *IPGM *EXEC *INTERACT *BATCH *BREXX *IREXX *BMOD *IMOD) CURLIB(*NOCHG) PRDLIB(*NOCHG)"

